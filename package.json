{
  "name": "psy",
  "description": "An psychoanalytical calculator for various assessments and an scanner for automatic analysis",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "^18.16.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "prisma generate && nuxt prepare",
    "start": "node .output/server/index.mjs",
    "cleanup": "nuxt cleanup",
    "test": "vitest",
    "database": "cockroach start-single-node --insecure --listen-addr=localhost --store=database",
    "seed": "prisma db seed",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:apply": "prisma migrate dev",
    "studio": "prisma studio"
  },
  "prisma": {
    "seed": "npx ts-node --project prisma/tsconfig.json prisma/scripts/seed.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@nuxt/devtools": "latest",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/google-adsense": "^3.0.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@pinia/nuxt": "^0.4.11",
    "@tresjs/nuxt": "^1.1.9",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.2",
    "@types/three": "^0.156.0",
    "@vite-pwa/nuxt": "^0.1.1",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.7.4",
    "nuxt-gtag": "^1.1.1",
    "nuxt-icons": "^3.2.1",
    "nuxt-schema-org": "^3.0.1",
    "prisma": "^5.4.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@nuxt/test-utils": "^3.7.4",
    "@prisma/client": "^5.4.1",
    "@splidejs/splide-extension-grid": "^0.4.1",
    "@splidejs/vue-splide": "^0.6.12",
    "canvas": "^2.11.2",
    "three": "^0.157.0",
    "uqr": "^0.1.2",
    "vee-validate": "^4.11.8",
    "vitest": "^0.33.0"
  }
}
